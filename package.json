{
  "name": "skunkworks",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsc && node ./bin/www",
    "debug": "tsc && ENV=dev DEBUG=true node ./bin/www",
    "test:unit": "ENV=dev env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "test:functional": "ENV=dev env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/functional/**/*.ts'"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "joi": "^17.5.0",
    "mongodb": "^4.2.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/joi": "^17.2.3",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.11",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.4.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^9.1.3",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
